<ManagerPage>:
    name: 'ManagerPage'                   #name of the page

    canvas:
        Rectangle:                          #essentialy is the background
            pos: self.pos                   #placed on screen
            size: self.size                 #size of screen
            source: 'images/managerbg.jpg'

    Button:
        id: Staff                                                                               #name of button
        text: 'Staff'                                                                           #text in button
        pos: (root.size[0] / 2) - (self.size[0] / 2), ((root.size[1] / 2) - (self.size[1] / 2)) #middle of screen
        size_hint: (None, None)                                                                 #allows resize 
        size: 200, 30                                                                           #same size as passord box
        on_press: 
            app.root.current = 'ManagerInterfacePage'                                           #Goes to Interface page